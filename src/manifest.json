{
    "manifest_version": 2,
    "name": "twitter画像原寸表示",
    "version": "1.5.1",
 
    "description": "twitterのツイート詳細画面にボタンを追加する拡張機能。追加されたボタンを押すとtwitterの画像の原寸を別ウィンドウで表示する。連絡は@HOGExtendまで。",
 
    "icons": {
        "16": "icons/icon.png",
        "48": "icons/icon.png",
        "128": "icons/icon.png"
    },
 
    "content_scripts": [
        {
            "matches": ["https://twitter.com/*"],
            "js": ["js/main.js"]
        }
    ]
}

