{
  "name": "twitter-open-original-image",
  "description": "opens images in original size at twitter",
  "repository": "git@github.com:hogashi/twitterOpenOriginalImage.git",
  "author": "hogashi <azuma.ginkakuji@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "run-s build:js build:userjs",
    "build:js": "webpack --version && webpack --mode production",
    "build-dev": "run-s build-dev:js build:userjs",
    "build-dev:js": "webpack --version && webpack --mode development",
    "build:userjs": "./scripts/make_user_script.sh",
    "lint": "run-s lint:prettier lint:eslint",
    "lint:eslint": "eslint --version && eslint src __tests__ --ext ts --ext tsx --ext js",
    "lint:prettier": "prettier --version && prettier --write 'src/**/*.ts{,x}' '__tests__/**/*.js'",
    "jest": "jest --version && jest --verbose --coverage",
    "coverage": "make-coverage-badge",
    "jest:ci": "jest --version && jest --verbose --coverage && cat ./coverage/lcov.info | coveralls",
    "test": "run-s lint:eslint test:ts jest coverage",
    "test:ts": "tsc --noEmit",
    "test:ci": "run-s lint:eslint jest:ci"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "@babel/preset-react": "7.9.4",
    "@babel/preset-typescript": "7.9.0",
    "@types/chrome": "0.0.93",
    "@types/jest": "25.2.1",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.6",
    "@types/react-test-renderer": "16.9.2",
    "@typescript-eslint/eslint-plugin": "2.18.0",
    "@typescript-eslint/parser": "2.18.0",
    "babel-loader": "8.1.0",
    "coveralls": "3.0.9",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "jest": "25.3.0",
    "make-coverage-badge": "1.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.4",
    "react-test-renderer": "16.13.1",
    "typescript": "3.8.3",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10"
  },
  "dependencies": {
    "@babel/polyfill": "7.8.7",
    "@material-ui/core": "3.9.4",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "jest": {
    "coverageReporters": [
      "json-summary",
      "lcov",
      "text"
    ]
  }
}
