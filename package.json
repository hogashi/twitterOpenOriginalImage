{
  "name": "twitter-open-original-image",
  "description": "opens images in original size at twitter",
  "repository": "git@github.com:hogashi/twitterOpenOriginalImage.git",
  "author": "hogashi <azuma.ginkakuji@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "run-s build:js build:userjs",
    "build:js": "webpack --version && webpack --mode production",
    "build-dev": "run-s build-dev:js build:userjs",
    "build-dev:js": "webpack --version && webpack --mode development",
    "build:userjs": "./scripts/make_user_script.sh",
    "lint": "run-s lint:prettier lint:eslint",
    "lint:eslint": "eslint --version && eslint src __tests__ --ext ts --ext tsx --ext js",
    "lint:prettier": "prettier --version && prettier --write 'src/**/*.ts{,x}' '__tests__/**/*.js'",
    "jest": "jest --version && jest --verbose --coverage",
    "coverage": "make-coverage-badge",
    "test": "run-s lint:eslint test:ts jest coverage",
    "test:ts": "tsc --noEmit",
    "compress": "zip -r dist.zip dist"
  },
  "devDependencies": {
    "@babel/cli": "7.12.1",
    "@babel/core": "7.12.3",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-react": "7.12.1",
    "@babel/preset-typescript": "7.12.1",
    "@tsconfig/node14": "^1.0.0",
    "@types/chrome": "0.0.125",
    "@types/jest": "26.0.15",
    "@types/react": "16.9.55",
    "@types/react-dom": "16.9.9",
    "@types/react-test-renderer": "16.9.3",
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.5.0",
    "babel-loader": "8.1.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "eslint": "7.12.0",
    "eslint-config-prettier": "6.14.0",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "jest": "26.6.1",
    "make-coverage-badge": "1.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "react-test-renderer": "17.0.1",
    "typescript": "4.0.3",
    "webpack": "5.2.0",
    "webpack-cli": "4.1.0"
  },
  "dependencies": {
    "@babel/polyfill": "7.12.1",
    "@babel/runtime": "7.12.1",
    "@material-ui/core": "4.11.0",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "jest": {
    "coverageReporters": [
      "json-summary",
      "lcov",
      "text"
    ]
  }
}
